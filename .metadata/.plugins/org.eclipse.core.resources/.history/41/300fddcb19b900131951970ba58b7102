	enum ShipEngine {STEAM_ENGINE, STEAM_TURBINE, GAS_TURBINE, DIESEL, 
		ELECTRIC, WIND, HUMAN_FORCE};

		public static void main(String [] args){
			myMethodWithVariableArguments("One", "Two", "Three");
		}
		public static void myMethodWithVariableArguments(String ... myOneArrayParameterForVariableArguments)
		{
			for(int i=0; i<myOneArrayParameterForVariableArguments.length; i++)
				System.out.println(myOneArrayParameterForVariableArguments[i]);
		}
}



