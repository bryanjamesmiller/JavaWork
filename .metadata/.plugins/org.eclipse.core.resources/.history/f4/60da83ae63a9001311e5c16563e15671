
public class ExtraCredit 
{
	public static void main(String [] args)
	{	String test = args[0];

	System.out.print("t");	
	
	try 
	{
		doRisky(test);
	}
	catch(MyException e){
	} finally {
		System.out.print("w");
		System.out.print("s");
	}



	}

	static void doRisky(String arg) throws MyException
	{ 	System.out.print("h");


	if ("Obama".equals(arg))
		throw new MyException();




	System.out.print("r");
	System.out.print("o");
	System.out.print("a");





	}







}

class MyException extends Exception {}
public class ExcExample{}